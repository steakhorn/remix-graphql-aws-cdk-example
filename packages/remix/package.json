  "scripts": {
    "build": "npm run codegen && npm run build:css && remix build",
    "build:css": "tailwindcss -o ./app/tailwind.css --minify",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -o ./app/tailwind.css --watch",
